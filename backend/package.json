{
    "name": "swift-bank-backend",
    "version": "1.0.0",
    "description": "Industry-standard Express.js backend for Swift Banking application",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
        "lint": "eslint src/**/*.ts",
        "type-check": "tsc --noEmit",
        "seed": "ts-node -r tsconfig-paths/register src/seeders/account-seeder.ts"
    },
    "keywords": [
        "banking",
        "express",
        "typescript",
        "mongodb",
        "rest-api"
    ],
    "author": "mahfuz",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.6.7",
        "bcrypt": "^5.1.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.4.0",
        "ejs": "^3.1.10",
        "express": "^5.1.0",
        "helmet": "^7.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^8.1.0",
        "nodemailer": "^7.0.6",
        "reflect-metadata": "^0.1.13",
        "tsyringe": "^4.8.0",
        "zod": "^3.22.4"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/cookie-parser": "^1.4.6",
        "@types/cors": "^2.8.17",
        "@types/ejs": "^3.1.5",
        "@types/express": "^4.17.21",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^20.19.19",
        "@types/nodemailer": "^7.0.2",
        "nodemon": "^3.0.3",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
    }
}
