{
    "rewrites": [
        {
            "source": "/api/v1/:path*",
            "destination": "https://api-swiftbank.vercel.app/api/v1/:path*"
        },
        {
            "source": "/api/v1/:path*/",
            "destination": "https://api-swiftbank.vercel.app/api/v1/:path*"
        },
        {
            "source": "/:path*",
            "destination": "/"
        }
    ]
}
